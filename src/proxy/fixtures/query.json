{
  "type": "file",
  "$or": [
    {
      "name": {
        "$match": "*.jar"
      }
    },
    {
      "name": {
        "$match": "*.tar.gz"
      }
    },
    {
      "name": {
        "$match": "*.zip"
      }
    }
  ],
  "$or": [
    {
      "stat.downloaded": {
        "$lt": "${thresholdTime}"
      }
    }
  ],
  "repo": {
    "$match": "${filter}"
  }
}
