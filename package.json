{
  "name": "artifactory-cleanup",
  "description": "Artifactory Cleanup",
  "version": "0.1.2",
  "author": "Gabriel Kohen (https://github.com/gkohen)",
  "repository": "https://github.com/JDASoftwareGroup/artifactory-cleanup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JDASoftwareGroup/artifactory-cleanup/issues"
  },
  "files": [
    "LICENSE"
  ],
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5"
  },
  "scripts": {
    "test": "TZ='Antarctica/Mawson' jest",
    "test:coverage": "npm run test -- --coverage",
    "test:coverage:deploy": "npm run test:coverage && codecov",
    "clean": "rimraf ./dist && rimraf ./coverage",
    "postinstall": "npm run build",
    "build": "babel src --out-dir dist --ignore Procfile,node_modules,test,*.spec.js,*.sh --no-comments --copy-files",
    "lint": "eslint src/**",
    "precommit": "lint-staged && npm run test",
    "start": "node ./dist",
    "dev": "NODE_ENV=development nodemon index.js --exec babel-node -- --help",
    "prepublish": "npm run build",
    "release": "standard-version"
  },
  "bin": {
    "artifactory-cleanup": "./bin/artifactory-cleanup.js"
  },
  "keywords": [
    "artifactory",
    "cleanup"
  ],
  "jestSonar": {
    "reportPath": "coverage"
  },
  "eslintIgnoreConfig": [
    "node_modules/",
    "coverage/",
    "dist/"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "parser": "flow"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write \"src/**/*.js\"",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "0.18.0",
    "axios-mock-adapter": "1.15.0",
    "dotenv": "6.2.0",
    "error-ex": "1.3.2",
    "figlet": "1.2.1",
    "filesize": "3.6.1",
    "inquirer": "6.2.1",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "winston": "3.1.0",
    "yargs": "12.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "acorn": "^6.0.4",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-minify": "0.5.0",
    "babel-preset-env": "1.7.0",
    "babel-watch": "2.0.7",
    "chalk": "2.4.1",
    "codecov": "^3.1.0",
    "eslint": "5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest-cli": "23.6.0",
    "jest-mock-console": "^0.4.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^8.1.0",
    "nodemon": "1.18.7",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2",
    "standard-version": "^4.4.0"
  }
}
